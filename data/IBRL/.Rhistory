showNotification(paste("En la limpieza de datos se han generado ", totalErr, " errores. Para verlos descárguese el fichero log.txt.", sep = ""), duration = 5, closeButton = TRUE, type = "warning")
else if(totalWarnings > 0)
showNotification(paste("En la limpieza de datos se han generado ", totalWarnings, " warnings. Para verlos descárguese el fichero log.txt.", sep = ""), duration = 5, closeButton = TRUE, type = "warning")
else{
# DESACTIVAR BOTON!!!!!!!!!!
}
if(nrow(infoViajes) > 0)
names(infoViajes) <- c("Matrícula", "Fecha", "Duración (horas)")
tablaDuraciones(infoViajes)
return(infoViajes)
})
